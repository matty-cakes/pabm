{
  "name": "pabm",
  "version": "1.0.0",
  "description": "pretty algorithms by matt",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "fmt": "prettier . --write --plugin prettier-plugin-svelte .",
    "style-lint": "stylelint './src/**/*.{css,svelte}'",
    "style-fix": "stylelint --fix './src/**/*.{css,svelte}'",
    "test": "vitest run",
    "test-watch": "vitest",
    "coverage": "vitest run --coverage",
    "build": "pnpm fmt && pnpm coverage && vite build",
    "prod-deploy": "flyctl deploy",
    "pipeline": "pnpm fmt && pnpm coverage && pnpm build && pnpm prod-deploy",
    "pipe": "pnpm pipeline"
  },
  "keywords": [
    "app",
    "algorithms",
    "love"
  ],
  "author": "Matty Cakes",
  "license": "ISC",
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/svelte": "^4.0.3",
    "@testing-library/user-event": "^14.5.1",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "stylelint": "^15.10.3",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard": "^34.0.0",
    "svelte": "^4.2.1",
    "svelte-spa-router": "^3.3.0",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.9",
    "vitest": "^0.34.6"
  }
}
